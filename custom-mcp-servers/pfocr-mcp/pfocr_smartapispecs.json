            }
          ],
          "requestBody": {
            "content": {
              "application/json": {
                "example": {
                  "q": [
                    "D045169",
                    "D005355"
                  ],
                  "scopes": [
                    "associatedWith.mentions.diseases.mesh"
                  ]
